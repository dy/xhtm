{
  "name": "xhtm",
  "version": "0.0.5",
  "description": "HTML Tagged Template syntax for Virtual DOM. Extension of HTM with HTML support.",
  "main": "dist/xhtm.js",
  "umd:main": "dist/xhtm.umd.js",
  "module": "dist/xhtm.module.mjs",
  "source": "src/index.js",
  "scripts": {
    "start": "parcel serve --no-hmr test/index.html",
    "test": "node -r esm test/index.test.js",
    "start:bench": "parcel serve --no-hmr test/bench.html",
    "build": "microbundle build --no-sourcemap -f modern --target web"
  },
  "repository": "dy/xhtm",
  "keywords": [
    "Hyperscript Tagged Markup",
    "tagged template",
    "template literals",
    "html",
    "htm",
    "jsx",
    "virtual dom",
    "hyperscript"
  ],
  "browserslist": [
    "last 1 Chrome versions"
  ],
  "author": "Dmitry Yv <df.creative@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/dy/xhtm",
  "devDependencies": {
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-modules": "^0.1.1",
    "common-tags": "^1.8.0",
    "domtagger": "^0.5.11",
    "esm": "^3.2.25",
    "htm": "^2.2.1",
    "html-template-string": "^1.2.6",
    "hyperx": "^2.5.4",
    "lit-html": "^1.1.2",
    "microbundle": "^0.12.0-next.6",
    "nanohtml": "^1.9.0",
    "tape": "^4.11.0"
  },
  "dependencies": {}
}
